<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="status"
            type="String" />
    </data>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:text="@{status}"
        android:textSize="14sp" />
</layout>
